package spring;

public class Client2 {
	
	private String host;

	public Client2(String host) {
		this.host = host;
		System.out.println("Client2.setHost() call : " + host);
	}
	
	public void connect() throws Exception{
		System.out.println("Client2.connect() 실행 - 초기화 메소드");	
	}
	
	public void send() {
		System.out.println("Client2.send() call : " + host);
	}
	
	public close() throws Exception{
		System.out.println("Client2.closer() call - 소멸 메소드");
	}
 	
	
}
