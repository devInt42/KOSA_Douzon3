package ex02.quiz;

public class StudentViewImpl implements IPersonView {
	private PersonImpl person;
	
	//setter로 주입
	public void setPerson(PersonImpl person) {
		this.person = person;
	}
	public StudentImpl(String name, String age, String sex, String job) {
		super();
		this.name = name;
		this.age = age;
		this.sex = sex;
		this.job = job;
	}
	
	@Override
	public void print() {
		System.out.println("이름 : " + 	person.getName());
		System.out.println("나이 : " + 	person.getAge());
		System.out.println("성별 : " + 	person.getSex());
		System.out.println("직업 : " + 	person.getJob());
	}

}
