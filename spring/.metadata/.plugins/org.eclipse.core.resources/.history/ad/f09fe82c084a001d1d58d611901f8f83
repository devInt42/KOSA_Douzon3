package ex02.quiz;

import java.util.Scanner;

public class StudentViewImpl extends PersonImpl{
	private PersonImpl person;
	private String age, name, sex, school;
	
	//setter로 주입

	public void Input() {
		setSchool("삼육대학교");
	}
	
	@Override
	public void print() {
		System.out.println("직업 : " + 	school);
		System.out.println("이름 : " + super.getName());
		System.out.println("나이 : " + super.getAge());
		System.out.println("성별 : " + super.getSex());
	}

	public String getSchool() {
		return school;
	}

	public void setSchool(String school) {
		this.school = school;
	}

}
