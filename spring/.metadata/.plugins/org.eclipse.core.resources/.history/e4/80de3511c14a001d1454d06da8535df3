package ex01.di.annot;

import java.util.ArrayList;

import org.springframework.context.annotation.Configuration;

@Configuration
public class JavaConfig {

	public MyCats cat1() {
		ArrayList<String> hobbys = new ArrayList();
		hobbys.add("앞발들기");
		
		MyCats myCats = new MyCats("냥이", 12, "");
		
		return myCats;
	}
}
